{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "plugin:prettier/recommended",
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
				"prettier"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/react-in-jsx-scope": "off",
        "max-len": "off",
				"prettier/prettier": ["error"]
    }
}
